<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="se.liu.ida.josfa969.testmodule.Main"
    tools:ignore="MergeRootFrame" >

    <fragment android:name="se.liu.ida.josfa969.testmodule.HeadlinesFragment"
        android:id="@+id/headlines_fragment"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        tools:layout="@layout/headlines_fragment" />

    <fragment android:name="se.liu.ida.josfa969.testmodule.ArticleFragment"
        android:id="@+id/article_text"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_gravity="right"
        tools:layout="@layout/article_fragment" />

    </LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.liu.ida.josfa969.lab2" >

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:largeScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:configChanges="orientation"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="se.liu.ida.josfa969.lab2.GroupListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="se.liu.ida.josfa969.lab2.AdditionalInfoActivity"
            android:label="@string/title_activity_additional_info"
            android:parentActivityName=".GroupListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GroupListActivity"/>
        </activity>
    </application>

</manifest>
